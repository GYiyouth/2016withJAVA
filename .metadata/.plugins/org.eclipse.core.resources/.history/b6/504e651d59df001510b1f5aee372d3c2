import java.util.Arrays;

public class Main {

	public static void main(String[] args) {
		int[] arr1 = new int [10];
		for(int i =0;i<arr1.length;i++)
		{
			arr1[i]=i;
		System.out.println(arr1[i]);
	
		}
		int []arr2 = Arrays.copyOfRange(arr1, 0, 3); // Êý×é¸´ÖÆ
		for(int ret:arr2)
			System.out.print(ret+" ");
	}
}
